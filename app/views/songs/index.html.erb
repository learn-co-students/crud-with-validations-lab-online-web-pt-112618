<h1>All Songs</h1>

<% @songs.each do |song|  %>
<table>
  <thead>
    <tr>
      <th>Song Title: </th>
      <th>Released: </th>
      <th>Released Year: </th>
      <th>Artist Name: </th>
      <th>Genre: </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
    </tr>
  </tbody>
</table>
<% end %>
